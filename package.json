{
  "name": "journo",
  "version": "0.0.1",
  "author": "crass_sandwich",
  "private": true,
  "scripts": {
    "ink": "./lib/inklecate -p src/story/main.ink",
    "compileInk": "./lib/inklecate -o src/story/content.json src/story/main.ink",
    "start": "react-scripts start  &  npx onchange -i src/**/*.ink -- npm run compileInk",
    "build": "npm run compileInk  &&  react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.5.1",
    "inkjs": "^1.8.0-beta.1",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "latest",
    "react-scrollable-feed": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "onchange": "^6.1.0",
    "watch": "^1.0.2"
  }
}
